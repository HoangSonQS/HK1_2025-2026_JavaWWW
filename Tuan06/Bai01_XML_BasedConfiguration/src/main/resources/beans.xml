<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student1" class="iuh.fit.se.bai01.beans.Student">
        <property name="id" value="001"/>
        <property name="name" value="Nguyen Van A"/>
    </bean>
    <bean id="class1" class="iuh.fit.se.bai01.beans.Class_">
        <property name="classId" value="DHKTPM17C"/>
        <property name="className" value="Dai Hoc KTPM17C"/>
    </bean>

    <bean id="student2" class="iuh.fit.se.bai01.beans.Student">
        <property name="id" value="002"/>
        <property name="name" value="Nguyen Van B"/>
        <property name="class_" ref="class1"/>
    </bean>
    <bean id="student3" class="iuh.fit.se.bai01.beans.Student">
        <constructor-arg name="id" value="003"/>
        <constructor-arg name="name" value="Nguyen Van Anh"/>
        <constructor-arg name="class_" ref="class1"/>
    </bean>
</beans>